Domain:
name:       <%= json['name'] %>
<%- for st in Array(json['status']) -%>
status:     <%= st %>
<%- end -%>
registered: <%= l json['registered'] %>
changed:    <%= l json['changed'] %>
expire:     <%= l json['expire'] %>
outzone:    <%= l json['outzone'] %>
delete:     <%= l json['delete'] %>

Registrant:
name:       <%= ip_in_whitelist ? json['registrant'] : 'Private Person' %>
email:      <%= ip_in_whitelist ? json['email'] : 'Not Disclosed' %>
changed:    <%= ip_in_whitelist ? l(json['registrant_changed']) : 'Not Disclosed' %>

<%- if json['admin_contacts'].present? -%>
Administrative contact:
<%- for contact in json['admin_contacts'] -%>
name:       <%= ip_in_whitelist ? contact['name'] : 'Not Disclosed' %>
email:      <%= ip_in_whitelist ? contact['email'] : 'Not Disclosed' %>
changed:    <%= ip_in_whitelist ? l(contact['changed']) : 'Not Disclosed' %>
<%- end -%>
<%- end -%>

<%- if json['tech_contacts'].present? -%>
Technical contact:
<%- for contact in json['tech_contacts'] -%>
name:       <%= ip_in_whitelist ? contact['name'] : 'Not Disclosed' %>
email:      <%= ip_in_whitelist ? contact['email'] : 'Not Disclosed' %>
changed:    <%= ip_in_whitelist ? l(contact['changed']) : 'Not Disclosed' %>
<%- end -%>
<%- end -%>

Registrar:
name:       <%= json['registrar'] %>
url:        <%= json['registrar_website'] %>
phone:      <%= json['registrar_phone'] %>
changed:    <%= l json['registrar_changed'] %>

<%- if json['nameservers'].present? -%>
Name servers:
<%- for server in json['nameservers'] -%>
nserver:   <%= server %>
<%- end -%>
changed:   <%= l json['nameservers_changed'] %>

<%- end -%>
<%- if json['dnssec_keys'].present? -%>
DNSSEC:
<%- for key in json['dnssec_keys'] -%>
dnskey:   <%= key %>
<%- end -%>
changed:  <%= l json['dnssec_changed'] %>
<%- end -%>
